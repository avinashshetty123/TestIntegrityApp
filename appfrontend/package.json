{
  "name": "appfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:main": "tsc --project tsconfig.electron.json",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run build:main && electron dist-electron/main.js\"",
    "electron:build": "vite build --config electron.vite.config.ts && electron-builder"
  },
  "main": "dist-electron/main.js",
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "face-api.js": "^0.22.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "electron": "^37.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  }
}
